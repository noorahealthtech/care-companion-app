	<array>
		<string>Assistant-Bold.otf</string>
		<string>Assistant-Regular.otf</string>
		<string>Assistant-ExtraBold.otf</string>
		<string>Assistant-SemiBold.otf</string>
		<string>AntDesign.ttf</string>
		<string>Entypo.ttf</string>
